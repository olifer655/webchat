.pageDetails {
  padding-bottom: 60rpx;
}
.title {
  border-left: 5rpx solid #007DD3;
}
.content {
  padding: 0 40rpx 30rpx 40rpx;
  border-bottom: 1rpx solid #E3E3E3;
  background-color: #fff;
  font-size: 30rpx;
  position: relative;
}
.negativeItem {
  display: flex;
  padding-top: 30rpx;
  justify-content: space-between;
  color: #464646;
}
.egItem {
  display: flex;
  padding-top: 30rpx;
}
.egItemKey {
  flex: 1;
  color: #9B9B9B;
}
.egItemValue {
  flex: 3;
  color: #464646;
}