.pageDetails {
  padding-bottom: 60rpx;
}
.title {
  border-left: 5rpx solid #00a2ff;
  
}
.content {
  padding: 0 40rpx 30rpx 40rpx;
  border-bottom: 1rpx solid #E3E3E3;
  border-top: 1rpx solid #E3E3E3;
  background-color: #fff;
  font-size: 30rpx;
  position: relative;
}
.contentnot {
  padding: 30rpx 40rpx 30rpx 40rpx;
}
.contentno {
  border-bottom: 1rpx solid #E3E3E3;
  border-top: 1rpx solid #E3E3E3;
  background-color: #fff;
  font-size: 30rpx;
  position: relative;
}
.negativeItem {
  display: flex;
  padding-top: 30rpx;
  justify-content: space-between;
  color: #464646;
}
.egItem {
  display: flex;
  padding-top: 30rpx;
}
.egItemKey {
  flex: 1;
  color: #9B9B9B;
}
.egItemValue {
  flex: 3;
  color: #464646;
}
.subTitle {
  color: #464646;
  font-size: 30rpx;
  padding-top: 30rpx;
}
.block {
  border-bottom: 1rpx solid #e6e6e6;
  padding: 0 40rpx 30rpx 0rpx;
  margin-left: 40rpx;
}
.block:last-child {
  border-bottom: 0rpx solid #e6e6e6;
}
.sub {
  color: #999;
}
.descript {
  margin-top: 30rpx;
  color: #999;
  font-size: 28rpx;
}
.bad {
  color: #FF8100;
}