.bills {
  margin-top: 29rpx;
  color: #464646;
}
.up {
  margin-bottom: 26rpx;
  font-weight: 500;
}
.name {
  color: #464646;
  font-size: 34rpx;
}
.price {
  color: #FF7700;
  font-size: 36rpx;
}
.date {
  color: #999999;
  font-size: 28rpx;
}
.refunding {
  color: #35C8D0;
  font-size: 28rpx;
}
.refunded {
  color: #999;
  font-size: 28rpx;
}
.empty {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #ccc;
  background-color: #fff;
  height: 99vh;
}
.emptyImage {
  height: 150rpx;
  width: 127rpx;
}
.emptyText {
  font-size: 28rpx;
  color: #999999;
  margin-top: 40rpx;
}
.loading {
  display: flex;
  margin: 40rpx auto 80rpx;
  color: #999;
  font-size: 28rpx;
  justify-content:center;
}